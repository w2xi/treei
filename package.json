{
  "name": "treei",
  "version": "1.2.9",
  "description": "a directory structure tree generator",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "url": "https://github.com/w2xi/treei"
  },
  "homepage": "https://github.com/w2xi/treei#readme",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "nr build",
    "dev": "tsup src/index.ts --watch",
    "build": "nr type && tsup src/index.ts --minify",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src --fix",
    "test": "vitest",
    "type": "tsc --noEmit"
  },
  "bin": {
    "treei": "dist/index.js"
  },
  "keywords": [
    "directory",
    "tree",
    "tree structure"
  ],
  "files": [
    "dist"
  ],
  "author": "w2xi",
  "license": "MIT",
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "bumpp": "^9.4.0",
    "eslint": "^8.56.0",
    "prettier": "3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "picocolors": "^1.1.1"
  }
}
